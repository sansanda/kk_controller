      "format_elements_valid_options": ["VOLT", "CURR"],
      "measure_function": "voltage",
      "measure_function_valid_options": ["voltage", "current", "resistance"],
      "measure_range": "auto",
      "measure_range_valid_options": {
        "dc_voltage": ["auto", 0.1, 1, 10, 100],
        "dc_current": ["auto", 0.001, 0.01, 0.1, 1],
        "resistance": ["auto", 10, 100, 1000, 10000],
        "four_wire_resistance": ["auto", 10, 100, 1000, 10000]
      },
      "nplc": 10,
      "nplc_valid_options": [0.01, 0.1, 1, 10, 100],
      "front_rear": "front",
      "front_rear_valid_options": ["front", "rear"]
