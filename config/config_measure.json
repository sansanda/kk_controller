{
  "Visa": {
    "timeout_ms": 30000,
    "timeout_ms_min": 0,
    "timeout_ms_max": 30000,
    "backend": "@ni",
    "backend_valid_options": ["@ni", "@keysight", "@py", "@sim"]
  },
  "Instruments": {
    "ImpedanceAnalyzer": {
      "model": "E4990A",
      "gpib_addr": "25",
      "timeout_ms": 10000,
      "f_start": 99.9E+3,
      "f_stop": 100.1E+3,
      "n_points": 3,
      "vac_level": 0.5
    },
    "SourceMeter": {
