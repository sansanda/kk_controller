    "statistics_delay": {
      "metric": "st_dev",
      "metric_valid_options": ["last_measure", "st_dev", "mean"],
      "comparator": "LESS_THAN",
      "comparator_valid_options": ["LESS_THAN", "GREATER_THAN", "EQUAL_TO"],
      "timer_interval": 1.0,
      "statistic_function": "measure_sweep_voltage",
